    "    knn.fit(X_train, y_train) #fit the train data\n",
    "    y_pred = knn.predict(X_test)\n",
    "    accuracy = knn.accuracy(y_test, y_pred)\n",
    "    accuracies.append(accuracy)\n",
    "\n",
